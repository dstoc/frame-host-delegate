{
	"name": "frame-host-delegate",
	"displayName": "frame-host-delegate",
	"description": "Accepts workspace serving requests from a frame host... ðŸ¤”",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:frame-host-delegate.activate"
	],
	"browser": "./dist/web/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "frame-host-delegate.activate",
				"title": "Activate frame host delegate"
			}
		],
		"configuration": {
			"title": "frame-host-delegate",
			"properties": {
				"frame-host-delegate.frameUrl": {
					"type": "string",
					"default": "",
					"description": "Target URL for the frame host delegate."
				}
			}
		}
	},
	"scripts": {
		"test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
		"pretest": "npm run compile-web",
		"vscode:prepublish": "npm run package-web",
		"compile-web": "webpack",
		"watch-web": "webpack --watch",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
	},
	"devDependencies": {
		"@types/vscode": "^1.62.0",
		"@types/mocha": "^9.0.0",
		"eslint": "^8.1.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-web": "^0.0.15",
		"ts-loader": "^9.2.5",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0",
		"@types/webpack-env": "^1.16.2",
		"assert": "^2.0.0",
		"process": "^0.11.10"
	}
}